import (
	"context"

	"github.com/goplus/builder/internal/core"
)

var (
	p *core.Project
)

todo := context.TODO()

get "/code/:id", ctx => {
	id := ctx.param("id")
	cloudFile, _ := p.FileInfo(todo, id)
	ctx.json {
		"id": ctx.param("id"),
		"address":cloudFile.Address,
	}
}


conf := &core.Config{}
p, _ = core.New(todo, conf)

run ":8080"